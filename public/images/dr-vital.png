<svg width="200" height="200" viewBox="0 0 200 200" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="drVitalGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0066FF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0044CC;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background circle -->
  <circle cx="100" cy="100" r="90" fill="url(#drVitalGradient)"/>
  
  <!-- Stethoscope icon -->
  <g transform="translate(100, 100)">
    <!-- Stethoscope tube -->
    <path d="M-30 -20 Q-30 -40 -20 -50 Q-10 -60 0 -60 Q10 -60 20 -50 Q30 -40 30 -20" 
          stroke="white" stroke-width="3" fill="none"/>
    
    <!-- Stethoscope chest piece -->
    <circle cx="0" cy="-20" r="8" fill="white"/>
    <circle cx="0" cy="-20" r="4" fill="#0066FF"/>
    
    <!-- Doctor cross -->
    <line x1="-15" y1="-35" x2="15" y2="-35" stroke="white" stroke-width="2"/>
    <line x1="0" y1="-50" x2="0" y2="-20" stroke="white" stroke-width="2"/>
  </g>
  
  <!-- Text -->
  <text x="100" y="160" font-family="Arial, sans-serif" font-size="16" font-weight="bold" 
        text-anchor="middle" fill="white">Dr. Vital</text>
</svg>